<a  name="services"></a>
<div class="content-section-c">
  <div align="center">
    <div class="container">
      <div class="row">

        <hr align="center" class="section-heading-spacer2">
        <div class="clearfix"></div>
        <h3 class="section-heading">Excellara brings experience to the workplace.  We connect businesses looking for
          part-time help with an accomplished generation of professionals. <br></h3>
      </div>
      <div>
        <h2 style="color: deeppink">It's Free! Sign Up Today!</h2>
        <%= link_to "Professionals", user_signup_path, class: "btn btn-lg btn-primary" , style: :"width: 200px" %>
        <%= link_to "Companies", company_signup_path, class: "btn btn-lg btn-success" , style: :"width: 200px" %>
        <p></p>
      </div>
    </div>
  </div>
</div>


